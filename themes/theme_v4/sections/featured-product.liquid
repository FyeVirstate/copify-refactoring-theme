{%- liquid
  assign product = section.settings.product
  if product == null
    assign placeholder = true
  endif

  assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src'
  assign product_form_id = 'product-form-' | append: section.id

  assign quantity_break_block = section.blocks | map:'type' | where:'quantity_break' | join:','
  assign quantity_break_block = quantity_break_block | split:',' | size       
-%}

<product-info
  data-section="{{ section.id }}"
  data-product-id="{{ product.id }}"
  data-update-url="false"
  data-url="{{ product.url }}"
  {% if section.settings.image_zoom == 'hover' %}
    data-zoom-on-hover
  {% endif %}
>
  {{'pdp-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'section-featured-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
  {% unless section.settings.product.has_only_default_variant %}
    {{ 'component-product-variant-picker.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch.css' | asset_url | stylesheet_tag }}
    {{ 'component-swatch-input.css' | asset_url | stylesheet_tag }}
  {% endunless %}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}

  <script src="{{ 'pdp-main-product.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>

  {% comment %} TODO: assign `product.selected_or_first_available_variant` to variable and replace usage to reduce verbosity {% endcomment %}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  <div class="ProductBuild__variations" data-section-type="featured-product">
    <div class="main_product-custom">  

      <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col grid--2-col-tablet">

            {% comment %}MEDIA GALLERY CODE START HERE{% endcomment %}
              <div class="grid__item product__media-wrapper">
                {%- unless placeholder -%}
                  {% render 'product-media-gallery', product: product, variant_images: variant_images %}
                {%- else -%}
                  <div class="product__media-item">
                    <div
                      class="product-media-container global-media-settings gradient{% if section.settings.constrain_to_viewport %} constrain-height{% endif %}"
                      style="--ratio: 1.0; --preview-ratio: 1.0;"
                    >
                      {{ 'product-apparel-1' | placeholder_svg_tag: 'placeholder-svg' }}
                    </div>
                  </div>
                {%- endunless -%}
              </div>
            {% comment %}MEDIA GALLERY CODE END HERE{% endcomment %}

            <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
              <section
                id="ProductInfo-{{ section.id }}"
                class="product__info-container"
                data-section="{{ section.id }}"
                data-url="{{ product.url }}"
              >
                {%- for block in section.blocks -%}
                    {%- case block.type -%}
                      {%- when '@app' -%}

                      {% render block %}
                      
                        {%- when 'trustpilot_widget' -%}
                          {% comment %}TRUSTPILOT DESIGN CODE START HERE{% endcomment %}
                            {%- render 'pdp-trustpilot-widget', product:product, block:block, section:section -%}
                          {% comment %}TRUSTPILOT DESIGN CODE END HERE{% endcomment %}

                        {%- when 'badge_about_product'-%}
                          {% comment %}BADGE ABOUT DESIGN CODE START HERE{% endcomment %}
                            {%- render 'pdp-badge-widget', product:product, block:block, section:section -%}
                          {% comment %}BADGE ABOUT DESIGN CODE END HERE{% endcomment %}

                        {%- when 'title' -%}
                          <div class="product__title">
                            {%- assign title_size = section.settings.product_title_size | default: 'h2' -%}
                            <h2 class="{{ title_size }}">
                              {%- unless placeholder -%}
                                {{ product.title | escape }}
                              {%- else -%}
                                {{ 'onboarding.product_title' | t }}
                              {%- endunless -%}
                            </h2>
                          </div>

                        {%- when 'bullets_badges'-%}
                          {% comment %}BULLETS BADGES CODE START HERE{% endcomment %}
                            {%- render 'pdp-bullets-badges', product:product, block:block, section:section -%}
                          {% comment %}BULLETS BADGES CODE END HERE{% endcomment %}

                        {%- when 'divider'-%}
                          {% comment %}DIVIDER CODE START HERE{% endcomment %}
                            <div class="pdp__dividerSeperated"><hr style="margin:{{ block.settings.margin_top }}px 0px {{ block.settings.margin_bottom }}px 0px"></div>
                          {% comment %}DIVIDER CODE END HERE{% endcomment %}

                        {%- when 'guranteed_notice'-%}
                          {% comment %}GURANTEED NOTICE CODE START HERE{% endcomment %}
                            {%- render 'pdp-guranteed-notice', product:product, block:block, section:section -%}
                          {% comment %}GURANTEED NOTICE CODE END HERE{% endcomment %}   
                          
                        {%- when 'collapsible_tabs'-%}
                            {% comment %}COLLAPSIBLE TABS CODE START HERE{% endcomment %}
                              {%- render 'pdp-collapsible-tabs', product:product, block:block, section:section -%}
                            {% comment %}COLLAPSIBLE TABS END HERE{% endcomment %}

                        {%- when 'customer_appreciation'-%}
                          {% comment %}CUSTOMER APPRECIATION CODE START HERE{% endcomment %}
                            {%- render 'pdp-customer-appreciation', product:product, block:block, section:section -%}
                          {% comment %}CUSTOMER APPRECIATION CODE END HERE{% endcomment %} 

                        {% when 'video_reviews' %}
                          {% render 'testimonial-review-widget' , product:product, block:block, section:section%}
                          
                        {%- when 'bagdes_text'-%}
                          {% comment %}BADGES TEXT CODE START HERE{% endcomment %}
                            {%- render 'pdp-badges-text', product:product, block:block, section:section -%}
                          {% comment %}BADGES TEXT CODE END HERE{% endcomment %} 
                          
                        {%- when 'payment_icons'-%}
                          {% comment %}PAYMENT ICONS CODE START HERE{% endcomment %}
                            {%- render 'pdp-payment-icons', product:product, block:block, section:section -%}
                          {% comment %}PAYMENT ICONS CODE END HERE{% endcomment %}
                          
                        {%- when 'shipping_information'-%}
                          {% comment %}SHIPPING INFORMATION CODE START HERE{% endcomment %}
                            {%- render 'pdp-shipping-info', product:product, block:block, section:section -%}
                          {% comment %}SHIPPING INFORMATION CODE END HERE{% endcomment %}
                          
                        {%- when 'disclaimer_information'-%}
                          {% comment %}DISCLAIMER INFORMATION CODE START HERE{% endcomment %}
                            {%- render 'pdp-disclaimer-information', product:product, block:block, section:section -%}
                          {% comment %}DISCLAIMER INFORMATION CODE END HERE{% endcomment %}
                          
                        {%- when 'heading_header'-%}
                          {% comment %}HEADING HEADER CODE START HERE{% endcomment %}
                          {%- if block.settings.heading != blank or block.settings.badge != blank -%}
                            <div class="customHeading__header customHeading-{{ block.id }} {% if block.settings.bg__color %}ThisNo__bg{% endif %}">
                              <div class="customHeading__headerContent">
                                {%- if block.settings.divider_bf -%}<div class="divider-line"></div>{%- endif -%}
                                <div class="customHeading__header__txt">
                                  {%- if block.settings.badge != blank -%}<div class="customHeading__header__badge">{{ block.settings.badge }}</div>{%- endif -%}
                                  {%- if block.settings.heading != blank -%}<div class="customHeading__header__textwp">{{ block.settings.heading }}</div>{%- endif -%}
                                </div>
                                {%- if block.settings.divider -%}<div class="divider-line"></div>{%- endif -%}
                              </div>
                            </div>  
                            {%- style -%} 
                              body .customHeading-{{ block.id }}{
                                margin: {{ block.settings.margin_top }}px 0px {{ block.settings.margin_bottom }}px 0px;
                                {% if block.settings.require_bg == true %}
                                background: rgb(var(--color-innerHighlight));
                                border-radius: {{ block.settings.border_radius | append:'px' }};
                                {% endif %}
                                padding: {{ block.settings.pd_space_top }}px {{ block.settings.pd_space_rtl }}px {{ block.settings.pd_bottom_top }}px {{ block.settings.pd_space_lft }}px;
                              }
                              body .customHeading-{{ block.id }} .customHeading__header__textwp{
                                font-size: {{ block.settings.font_size }}px;
                                font-weight: {{ block.settings.font_weight }};
                                color: rgba(var(--color-foreground));
                              }
                              body .customHeading-{{ block.id }} .customHeading__headerContent .divider-line{
                                background-color: rgba(var(--color-foreground));
                              }                        
                              @media(max-width: 749px){
                                body .customHeading-{{ block.id }} .customHeading__header__textwp{
                                  font-size: {{ block.settings.font_size_mb }}px;
                                }                          
                              }
                            {%- endstyle -%} 
                          {%- endif -%}    
                          {% comment %}HEADING HEADER CODE END HERE{% endcomment %}       
                          
                        {%- when 'upsell'-%}
                          {% comment %}UPSELL CODE START HERE{% endcomment %}
                            {%- render 'pdp-upsell', product:product, block:block, section:section -%}
                          {% comment %}UPSELL CODE END HERE{% endcomment %}                      

                        {%- when 'product_labels'-%}
                          {% comment %}PRODUCT LABELS CODE START HERE{% endcomment %}
                            {%- render 'pdp-product-labels', product:product, block:block, section:section -%}
                          {% comment %}PRODUCT LABELS CODE END HERE{% endcomment %}
                          
                        {%- when 'product_labels_ii'-%}
                          {% comment %}PRODUCT LABELS CODE START HERE{% endcomment %}
                            {%- render 'pdp-product-labels-ii', product:product, block:block, section:section -%}
                          {% comment %}PRODUCT LABELS CODE END HERE{% endcomment %}                      
                          
                        {%- when 'free_premium_kit'-%}
                          {% comment %}FREE PREMIUM CODE START HERE{% endcomment %}
                            {%- render 'pdp-premium-kit', product:product, block:block, section:section -%}
                          {% comment %}FREE PREMIUM CODE END HERE{% endcomment %} 
                          
                        {%- when 'selling_out_bar'-%}
                          {% comment %}SELLING OUT BAR CODE START HERE{% endcomment %}
                            {%- render 'pdp-selling-out', product:product, block:block, section:section -%}
                          {% comment %}SELLING OUT BAR CODE END HERE{% endcomment %}                      

                        {%- when 'price_widget'-%}
                          {% comment %}BENEFITS CODE START HERE{% endcomment %}
                            {%- render 'pdp-price-widget', product:product, block:block, section:section -%}
                          {% comment %}BENEFITS CODE END HERE{% endcomment %}                      

                        {%- when 'variant_picker' -%}
                          {%- if quantity_break_block == 0 -%}
                          {% render 'pdp-variant-selector', product: product, block: block, product_form_id: product_form_id %}
                          {%- endif -%}

                        {%- when 'buy_buttons' -%}
                          {%- render 'pdp-buy-buttons',
                            block: block,
                            product: product,
                            product_form_id: product_form_id,
                            section_id: section.id,
                            show_pickup_availability: true,
                            quantity_break_block:quantity_break_block
                          -%}

                        {%- when 'quantity_break' -%}
                          {% comment %}QUANTITY BREAK CODE START HERE{% endcomment %}
                            {%- render 'pdp-quantity-break', product:product, block:block, section:section -%}
                          {% comment %}QUANTITY BREAK END HERE{% endcomment %}
                          

                    {%- endcase -%}
                  {%- endfor -%}

                <a
                  {% if product == blank %}
                    role="link" aria-disabled="true"
                  {% else %}
                    href="{{ product.url }}"
                  {% endif %}
                  class="link product__view-details animate-arrow"
                >
                  {{ 'products.product.view_full_details' | t }}
                  {{- 'icon-arrow.svg' | inline_asset_content -}}
                </a>
              </section>
            </div>
  
          {% render 'product-media-modal', product: product, variant_images: variant_images %}
     
      </div>

    </div>
  </div>

  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {%- if section.settings.image_zoom == 'hover' -%}
    <script id="EnableZoomOnHover-featured" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {%- endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- if first_3d_model -%}
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{ product.media | where: 'media_type', 'model' | json }}
    </script>
    <script src="{{ 'product-model.js' | asset_url }}" defer></script>
  {%- endif -%}

  {%- liquid
    if product.selected_or_first_available_variant.featured_media
      assign seo_media = product.selected_or_first_available_variant.featured_media
    else
      assign seo_media = product.featured_media
    endif
  -%}

  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>

  {% if product.media.size > 0 %}
    <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
  {% endif %}
</product-info>

{% javascript %}
  document.addEventListener('DOMContentLoaded', ()=>{
    const isIOS = /iPad|iPhone|iPod/.test(navigator.userAgent);
    const testimonialEl = document.querySelector('.Testimonial__reviews_interior.swiper');
    if (testimonialEl) {
      const testimonialConfig = { spaceBetween: 12, slidesPerView: 'auto', grabCursor: true };
      if (!isIOS) {
        testimonialConfig.mousewheel = { forceToAxis: true, thresholdDelta: 35 };
        testimonialConfig.freeMode = true;
      }
      new Swiper(testimonialEl, testimonialConfig);
    }

    if( document.querySelectorAll('.pdp__productFaq_widget__question').length > 0 ){
      document.querySelectorAll('.pdp__productFaq_widget__question').forEach((el)=>{
          el.addEventListener('click', (target)=>{
              target.preventDefault();
              target.target.closest('.pdp__productFaq_widget__item').classList.toggle('active');
          })
      })
    }
    if( document.querySelectorAll('.video__playIcon').length > 0 ){
      document.querySelectorAll('.video__playIcon .action--pause').forEach((elm)=>{
        elm.addEventListener('click', (target)=>{
          target.preventDefault();
          target.currentTarget.classList.add('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('.action--play').classList.remove('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('video').pause();
        })
      });
      document.querySelectorAll('.video__playIcon .action--play').forEach((elm)=>{
        elm.addEventListener('click', (target)=>{
          target.preventDefault();
          target.currentTarget.classList.add('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('.action--pause').classList.remove('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('video').play();
        })
      });  
    }
  });

  document.addEventListener("shopify:section:load", () => {
    const isIOS = /iPad|iPhone|iPod/.test(navigator.userAgent);
    const testimonialEl = document.querySelector('.Testimonial__reviews_interior.swiper');
    if (testimonialEl) {
      const testimonialConfig = { spaceBetween: 12, slidesPerView: 'auto', grabCursor: true };
      if (!isIOS) {
        testimonialConfig.mousewheel = { forceToAxis: true, thresholdDelta: 35 };
        testimonialConfig.freeMode = true;
      }
      new Swiper(testimonialEl, testimonialConfig);
    }

    if( document.querySelectorAll('.pdp__productFaq_widget__question').length > 0 ){
      document.querySelectorAll('.pdp__productFaq_widget__question').forEach((el)=>{
          el.addEventListener('click', (target)=>{
              target.preventDefault();
              target.target.closest('.pdp__productFaq_widget__item').classList.toggle('active');
          })
      })
    }
    if( document.querySelectorAll('.video__playIcon').length > 0 ){
      document.querySelectorAll('.video__playIcon .action--pause').forEach((elm)=>{
        elm.addEventListener('click', (target)=>{
          target.preventDefault();
          target.currentTarget.classList.add('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('.action--play').classList.remove('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('video').pause();
        })
      });
      document.querySelectorAll('.video__playIcon .action--play').forEach((elm)=>{
        elm.addEventListener('click', (target)=>{
          target.preventDefault();
          target.currentTarget.classList.add('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('.action--pause').classList.remove('hidden');
          target.target.closest('.Testimonial__video_tagged').querySelector('video').play();
        })
      });  
    }
  });
{% endjavascript %}

