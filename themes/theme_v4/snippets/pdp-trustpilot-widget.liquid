    {%- style -%}
        body .trustPilot__widget-wp-{{ block.id }}.design_mode_a{
            margin: {{ block.settings.margin_top }}px 0px {{ block.settings.margin_bottom }}px 0px;
        }
        body .trustPilot__widget-wp-{{ block.id }}.trustpilot__widgetLayout {
            border: 1px solid rgb(var(--color-primary));
        }
    {%- endstyle -%}    
{%- elsif block.settings.design_mode == 'design_b' -%}
    <div class="review__componentWidget design_mode_b trustPilot__widget-wp-{{ block.id }}">
        <div class="review__componentContainer">
            <div class="review__componentStars">
                <div class="review__componentStarsIcns">
                    {%- for i in (0..4) -%}
                        <svg class="review-star" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="M11.525 3.464a.5.5 0 0 1 .95 0l1.658 5.1a.5.5 0 0 0 .475.346h5.364a.5.5 0 0 1 .294.904l-4.34 3.153a.5.5 0 0 0-.181.559l1.657 5.1a.5.5 0 0 1-.77.56l-4.338-3.153a.5.5 0 0 0-.588 0l-4.339 3.153a.5.5 0 0 1-.77-.56l1.658-5.1a.5.5 0 0 0-.182-.56L3.734 9.815a.5.5 0 0 1 .294-.904h5.364a.5.5 0 0 0 .475-.346z" fill="#211a28"/></svg>
                    {%- endfor -%}
                </div>
                <div class="review__componentRated">Rated</div>
                {%- if block.settings.ratings_dsg_b != blank -%}<div class="review__componentratings">{{ block.settings.ratings_dsg_b }}</div>{%- endif -%}
                {%- if block.settings.ratings_count_dsg_b != blank -%}<div class="review__componentratingsCount">{{ block.settings.ratings_count_dsg_b }}</div>{%- endif -%}
            </div>
            {%- if block.settings.ratings_satisfaction_dsg_b != blank -%}<div class="review__componentSatisfaction">{{ block.settings.ratings_satisfaction_dsg_b }}</div>{%- endif -%}
        </div>
    </div>
    {%- style -%}
        body .trustPilot__widget-wp-{{ block.id }}.design_mode_b{
            margin: {{ block.settings.margin_top }}px 0px {{ block.settings.margin_bottom }}px 0px;
        }
        body .trustPilot__widget-wp-{{ block.id }} .review__componentSatisfaction{
            background-color: rgb(var(--color-innerHighlight));
            color: rgba(var(--color-foreground));
        }
    {%- endstyle -%}      
{%- elsif block.settings.design_mode == 'design_c' -%}
    <div class="review__componentWidget_rt design_mode_c trustPilot__widget-wp-{{ block.id }}">
        <div class="review__componentContainer_rt">
            <div class="review__componentStars_rt">
                <div class="review__componentStarsIcns_rt">
                    {%- for i in (0..4) -%}
                        <svg class="review-star" viewBox="0 0 24 24" xmlns="http://www.w3.org/2000/svg"><path d="M11.525 3.464a.5.5 0 0 1 .95 0l1.658 5.1a.5.5 0 0 0 .475.346h5.364a.5.5 0 0 1 .294.904l-4.34 3.153a.5.5 0 0 0-.181.559l1.657 5.1a.5.5 0 0 1-.77.56l-4.338-3.153a.5.5 0 0 0-.588 0l-4.339 3.153a.5.5 0 0 1-.77-.56l1.658-5.1a.5.5 0 0 0-.182-.56L3.734 9.815a.5.5 0 0 1 .294-.904h5.364a.5.5 0 0 0 .475-.346z" fill="#211a28"/></svg>
                    {%- endfor -%}
                </div>
                <div class="review__componentRated_rt_wp">
                    <div class="review__componentRated_rt">Excellent</div>
                    {%- if block.settings.ratings_dsg_c != blank -%}<div class="review__componentratings_rt">{{ block.settings.ratings_dsg_c }}</div>{%- endif -%}
                </div>
            </div>
            <div class="review__componentAvtar_rt">
            {%- if block.settings.ratings_avtar_dsg_c != blank
                or block.settings.ratings_avtarb_dsg_c != blank
                or block.settings.ratings_avtarc_dsg_c != blank
                or block.settings.ratings_avtard_dsg_c != blank
                or block.settings.ratings_avtare_dsg_c != blank
                -%}
                {%- if block.settings.ratings_avtar_dsg_c != blank -%}
                    <div class="review__componentAvtar_mg_t">{{ block.settings.ratings_avtar_dsg_c | image_url:width:block.settings.ratings_avtar_dsg_c.width | image_tag:preload:true }}</div>
                {%- endif -%}
                {%- if block.settings.ratings_avtarb_dsg_c != blank -%}
                    <div class="review__componentAvtar_mg_t">{{ block.settings.ratings_avtarb_dsg_c | image_url:width:block.settings.ratings_avtarb_dsg_c.width | image_tag:preload:true }}</div>
                {%- endif -%}
                {%- if block.settings.ratings_avtarc_dsg_c != blank -%}
                    <div class="review__componentAvtar_mg_t">{{ block.settings.ratings_avtarc_dsg_c | image_url:width:block.settings.ratings_avtarc_dsg_c.width | image_tag:preload:true }}</div>
                {%- endif -%}
                {%- if block.settings.ratings_avtard_dsg_c != blank -%}
                    <div class="review__componentAvtar_mg_t">{{ block.settings.ratings_avtard_dsg_c | image_url:width:block.settings.ratings_avtard_dsg_c.width | image_tag:preload:true }}</div>
                {%- endif -%}
                {%- if block.settings.ratings_avtare_dsg_c != blank -%}
                    <div class="review__componentAvtar_mg_t">{{ block.settings.ratings_avtare_dsg_c | image_url:width:block.settings.ratings_avtare_dsg_c.width | image_tag:preload:true }}</div>
                {%- endif -%}  
            {%- else -%}  
                {%- for i in (1..5) -%}
                    <div class="review__componentAvtar_mg_t">{{ 'avtar.svg' | inline_asset_content }}</div>
                {%- endfor -%}
            {%- endif -%}
            </div>
        </div>
    </div>  
    {%- style -%}
        body .trustPilot__widget-wp-{{ block.id }}.design_mode_c{
            margin: {{ block.settings.margin_top }}px 0px {{ block.settings.margin_bottom }}px 0px;
        }
    {%- endstyle -%}      
{%- endif -%}