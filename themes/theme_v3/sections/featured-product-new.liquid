                                  </div>
                                  <div class="toottip_content">
                                    <p><span>Color :</span> {{ value }}</p>
                                    <p class="soldout_msg">{{ sold_out_msg }}</p>
                                  </div>
                                </div>
                                {% if option_disabled %} {% endif %}
                              </div>
                            {% endfor %}
                          </fieldset>

                         
