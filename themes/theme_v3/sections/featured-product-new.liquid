<script>
(function () {
  // ---- helpers
  function formatMoney(cents) {
    try {
      return new Intl.NumberFormat("{{ shop.locale }}", {
        style: "currency",
        currency: "{{ shop.currency }}",
      }).format((cents || 0) / 100);
    } catch (e) {
      // very safe fallback
      return (cents / 100).toFixed(2);
    }
  }

  function getVariantFromPicker(pickerEl) {
    if (!pickerEl) return null;
    var jsonEl = pickerEl.querySelector('script[data-selected-variant]');
    if (!jsonEl) return null;
    try { return JSON.parse(jsonEl.textContent); } catch (e) { return null; }
  }

  function updateButtonText(spanEl, variant) {
    if (!spanEl) return;
    if (!variant) {
      spanEl.textContent = "{{ 'products.product.unavailable' | t }}";
      return;
    }
    if (!variant.available) {
      spanEl.textContent = "{{ 'products.product.sold_out' | t }}";
      return;
    }
    spanEl.textContent = "{{ 'products.product.add_to_cart' | t }}  " + formatMoney(variant.price);
  }

  document.addEventListener('DOMContentLoaded', function () {
    var sectionId = "{{ section_id }}";

    // target the *inner* span only (not the button). Important because the button also has class 'ram'
    var addToCartTextSpan = document.querySelector('#ProductSubmitButton-' + sectionId + ' span.ram');
    if (!addToCartTextSpan) return;

    // your variant-selects id uses section.id in the picker snippet
    var variantSelects = document.getElementById('variant-selects-' + sectionId);
    if (!variantSelects) {
      // fallback: first variant-selects in this section
      variantSelects = document.querySelector('variant-selects[id^="variant-selects-' + sectionId + '"]') || document.querySelector('variant-selects');
    }

    // initial set
    updateButtonText(addToCartTextSpan, getVariantFromPicker(variantSelects));

    // update on user changes
    if (variantSelects) {
      variantSelects.addEventListener('change', function () {
        updateButtonText(addToCartTextSpan, getVariantFromPicker(variantSelects));
      });

      // also observe for themes that rewrite the JSON without firing 'change'
      var observer = new MutationObserver(function () {
        updateButtonText(addToCartTextSpan, getVariantFromPicker(variantSelects));
      });
      observer.observe(variantSelects, { childList: true, subtree: true, characterData: true });
    }
  });
})();
</script>
<script>
  document.addEventListener('DOMContentLoaded', function () {
  const accordions = document.querySelectorAll('.accordion-item');

  accordions.forEach(item => {
    item.addEventListener('toggle', function () {
      if (this.open) {
        accordions.forEach(other => {
          if (other !== this) other.removeAttribute('open');
        });
      }
    });
  });
});

