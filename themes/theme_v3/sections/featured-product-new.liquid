    centerMode: false,
    centerPadding: '0px',
    slidesToShow: 3,
    variableWidth: true,
    draggable: true,            // <-- enables dragging on desktop
  swipeToSlide: true,         
    autoplay: false,
    arrows: true,
    dots: false,
    responsive: [
      {
        breakpoint: 768,
        settings: {
          slidesToShow: 2,
          centerPadding: '40px'
        }
      }
    ]
  });

  // Play button functionality
  $(".play-btn").on("click", function(){
    var video = $(this).siblings("video").get(0);

    // pause all other videos
    $(".video-slider video").each(function(){
      this.pause();
      $(this).siblings(".play-btn").show();
    });

    // play selected
    if(video.paused){
      video.play();
      $(this).hide();
    } else {
      video.pause();
      $(this).show();
    }
  });
});
</script>
